		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"globals": "^15.0.0",
		"mdsvex": "^0.11.2",
		"postcss": "^8.4.40",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"rehype-slug": "^6.0.0",
		"remark-toc": "^9.0.0",
		"remark-unwrap-images": "^4.0.0",
		"shiki": "^1.12.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.7",
