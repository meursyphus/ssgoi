{
  "name": "@ssgoi/core",
  "version": "0.0.1",
  "description": "A smooth page transition library",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./transitions": {
      "types": "./dist/transitions/index.d.ts",
      "import": "./dist/transitions.js",
      "require": "./dist/transitions.cjs"
    },
    "./view-transitions": {
      "types": "./dist/view-transitions/index.d.ts",
      "import": "./dist/view-transitions.js",
      "require": "./dist/view-transitions.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "page-transition",
    "animation",
    "typescript",
    "vanilla"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "popmotion": "^11.0.5"
  }
}
