{
  "name": "@ssgoi/qwik",
  "version": "1.0.0",
  "description": "Qwik bindings for SSGOI - Native app-like page transitions for Qwik applications",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib",
    "lib-types"
  ],
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib-types/index.d.ts",
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    },
    "./transitions": {
      "types": "./lib-types/transitions/index.d.ts",
      "import": "./lib/transitions/index.qwik.mjs",
      "require": "./lib/transitions/index.qwik.cjs"
    },
    "./view-transitions": {
      "types": "./lib-types/view-transitions/index.d.ts",
      "import": "./lib/view-transitions/index.qwik.mjs",
      "require": "./lib/view-transitions/index.qwik.cjs"
    },
    "./types": {
      "types": "./lib-types/types.d.ts",
      "import": "./lib/types.qwik.mjs",
      "require": "./lib/types.qwik.cjs"
    },
    "./presets": {
      "types": "./lib-types/presets/index.d.ts",
      "import": "./lib/presets/index.qwik.mjs",
      "require": "./lib/presets/index.qwik.cjs"
    }
  },
  "scripts": {
    "dev": "vite --mode ssr",
    "build": "vite build --mode lib && tsc --emitDeclarationOnly",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "watch": "vite build --mode lib --watch",
    "lint": "eslint .",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@ssgoi/core": "workspace:^"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^1.0.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.2",
    "@types/node": "^22.12.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "keywords": [
    "qwik",
    "page-transition",
    "animation",
    "ssgoi",
    "spring-physics"
  ],
  "author": "MeurSyphus <tmdeoans@snu.ac.kr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meursyphus/ssgoi.git",
    "directory": "packages/qwik"
  },
  "homepage": "https://ssgoi.dev",
  "bugs": {
    "url": "https://github.com/meursyphus/ssgoi/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
