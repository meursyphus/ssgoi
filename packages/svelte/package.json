{
	"name": "@ssgoi/svelte",
	"version": "0.0.7",
	"description": "Svelte bindings for SSGOI page transition library",
	"private": false,
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"scripts": {
		"dev": "svelte-package --watch",
		"build": "svelte-package",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@ssgoi/core": "workspace:*"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"keywords": [
		"svelte",
		"page-transition",
		"animation",
		"ssgoi",
		"spring-physics"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/ssgoi.git",
		"directory": "packages/svelte"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./transitions": {
			"types": "./dist/transitions/index.d.ts",
			"import": "./dist/transitions/index.js"
		},
		"./view-transitions": {
			"types": "./dist/view-transitions/index.d.ts",
			"import": "./dist/view-transitions/index.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.js"
		}
	}
}
